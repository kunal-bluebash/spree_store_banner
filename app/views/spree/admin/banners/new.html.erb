<% content_for :page_title do %>
  <%= link_to Spree.t(:banner), spree.admin_banners_url %> /
  <%= Spree.t(:new_banner) %>
<% end %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @banner } %>
<%= form_for [:admin, @banner] , url: spree.admin_banners_path  do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links', locals: { collection_url: spree.admin_banners_path(@banner) } %>
  </fieldset>
<% end %>
